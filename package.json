{
	"name": "vscode-wordpress-hooks",
	"displayName": "Autocomplete WordPress Hooks",
	"description": "Autocomplete provider for WordPress action and filter names",
	"version": "0.2.1",
	"license": "GPL-3.0-or-later",
	"publisher": "johnbillion",
	"author": {
		"name": "John Blackbourn"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Snippets"
	],
	"keywords": [
		"wordpress",
		"php",
		"autocomplete",
		"hooks",
		"actions",
		"filters"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/johnbillion/vscode-wordpress-hooks.git"
	},
	"bugs": {
		"url": "https://github.com/johnbillion/vscode-wordpress-hooks/issues"
	},
	"activationEvents": [
		"onLanguage:php"
	],
	"galleryBanner": {
		"color": "#0073aa",
		"theme": "dark"
	},
	"icon": "images/icon.png",
	"main": "./out/extension.js",
	"scripts": {
		"deploy": "vsce publish",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/node": "^10.17.0",
		"@types/vscode": "^1.32.0",
		"ts-loader": "^8.0.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"vsce": "^1.77.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"@johnbillion/wp-hooks": "^0.4.4"
	}
