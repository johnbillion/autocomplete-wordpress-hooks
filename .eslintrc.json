{
  "env": {
    "browser": false,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "indent": "off",
    "import/prefer-default-export": "off",
    "max-len":"off",
    "no-tabs": "off",
    "no-use-before-define": "off"
  }
}
